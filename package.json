{
  "name": "next-starter",
  "version": "0.1.7",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "eslint '{app,assets,components,constants,store,types}/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "lint:format": "prettier --write '{app,assets,components,constants,store,types}/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "version:patch": "yarn version --patch && yarn config set version-git-message \"chore(version): %s\"",
    "version:minor": "yarn version --minor && yarn config set version-git-message \"chore(version): %s\"",
    "version:major": "yarn version --major && yarn config set version-git-message \"chore(version): %s\"",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "test:coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^2.9.0",
    "@reduxjs/toolkit": "^1.9.3",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "lodash.omit": "^4.5.0",
    "lucide-react": "^0.129.0",
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.31.3",
    "react-redux": "^8.0.5",
    "tailwind-merge": "^1.10.0",
    "tailwindcss-animate": "^1.0.5",
    "whatwg-fetch": "^3.6.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@storybook/addon-actions": "^6.5.7",
    "@storybook/addon-essentials": "^6.5.7",
    "@storybook/addon-interactions": "^6.5.7",
    "@storybook/addon-links": "^6.5.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.7",
    "@storybook/manager-webpack5": "^6.5.7",
    "@storybook/react": "^6.5.7",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.2.1",
    "@types/lodash.omit": "^4.5.7",
    "@types/node": "17.0.41",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.2.1",
    "babel-loader": "^8.2.5",
    "eslint": "8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.5.12",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "tailwindcss": "^3.2.7",
    "typescript": "4.7.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:format",
      "yarn lint:check"
    ]
  }
}
